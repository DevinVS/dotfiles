{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "content_script.js" ],
      "matches": [ "https://*.wikipedia.org/wiki/*", "https://*.wikipedia.org/w/*" ],
      "run_at": "document_end"
   }, {
      "js": [ "ctrlf.js" ],
      "matches": [ "https://*.wikipedia.org/wiki/*", "https://*.wikipedia.org/w/*" ],
      "run_at": "document_start"
   } ],
   "description": "Play multiplayer Wikiracing online with your friends, directly from the Wikipedia Page!",
   "icons": {
      "128": "images/icon-128.png",
      "32": "images/icon-32.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Multiplayer Wikigame",
   "page_action": {
      "default_icon": {
         "128": "images/icon-128.png",
         "32": "images/icon-32.png"
      },
      "default_popup": "popup.html"
   },
   "permissions": [ "declarativeContent", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3.4",
   "web_accessible_resources": [ "images/header.png", "images/icon-32.png" ]
}
